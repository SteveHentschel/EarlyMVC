@model IEnumerable<MyMusicStore.Models.Genre>

<nav id="categories">
    <ul>
        @foreach (var genre in Model)
        {
            <li>
                @Html.ActionLink(genre.Name,
                    "Browse", "MyStore",
                    new { Genre = genre.Name }, null)
        </li>
        }

        <li>
            @Html.ActionLink("More...", "Index", "MyStore")
    </li>
</ul>
</nav>